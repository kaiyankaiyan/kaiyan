				</param>
			</property>
		</view>
		<view name="Store" label="库存导入" builder="EditViewBuilder">
			<property name="PurchaseTicketForm" class="PurchaseTicketForm" builder="ViewFieldBuilder">
				<param name="cfg">
					<attribute name="view-name" value="Import" />
